class Solution {
    public int missingNumber(int[] nums) {
        int a=-1;
        Arrays.sort(nums);
        int i=0;
        for(i=0;i<nums.length;i++){
            if(nums[i]!=i){
                return i;
            }
        }
        if(i==nums.length)
            return nums.length;
        return a;

    }
}
